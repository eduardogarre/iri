//Código de ejemplo
módulo código;

@hola = [6 x e32] ['h', 'o', 'l', 'a', ',', ' ', 'm', 'u', 'n', 'd', 'o', '.', '\n', '\0'];

define e32 @inicio(r32 %pi)
{
    %2 = sum e32 0, 0;
    %hola = ponval [6 x e32] @hola, e32 'H', 0;

bucle:
    %0 = leeval [6 x e32] %hola, %2;
    %2 = sum e32 %2, 1;
    llama nada @#poncar(%0);
    %1 = cmp dsig e32 %0, '\0';
    slt n1 %1, :bucle;

    ret e32 0;
}